{
  "compilerOptions": {
    "experimentalDecorators": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
      // "@root/*": ["*"],
      // "@utils/*": ["src/utils/*"],
      // "@reduxs/*": ["src/redux-relate/*"],
    },
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react"
  },
  "exclude": ["node_modules", "dist"],
  "include": ["./src/**/*"]
}
