{
  "extends": "react-app",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "eslintrc": true,
      "jsx": true,
      "js": true,
      "json": true,
      "legacyDecorators": true,
      "jest": true
    }
  },
  "rules": {
    "eqeqeq": "error",
    "curly": "error",
    "quotes": ["error", "single"],
    "comma-dangle": ["error", "always"],
    "semi": ["error", "never"]
  },
  "env": {
    "browser": true,
    "node": true
  }
}